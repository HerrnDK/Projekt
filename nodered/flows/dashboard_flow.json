    "func": "const ssid = (flow.get(\"selectedSsid\") || \"\").toString();\nconst pw = (flow.get(\"wifiPassword\") || \"\").toString();\nif (!ssid) return { payload: \"FEHLER: Bitte zuerst ein WLAN auswählen.\" };\nconst esc = (s) => s.replace(/\"/g, '\\\\\"');\nmsg.payload = `sudo /usr/local/bin/projekt-wifi-connect.sh \"${esc(ssid)}\" \"${esc(pw)}\"`;\nreturn msg;",

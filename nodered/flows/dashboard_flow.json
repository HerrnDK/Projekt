  {
    "id": "fn_eval_net",
    "type": "function",
    "z": "tab_pa_final",
    "name": "Netzstatus auswerten + QR Ziel bestimmen",
    "func": "const txt = (msg.payload || \"\").toString();\nconst lines = txt.split(/\\r?\\n/).filter(l => l.trim().length);\nlet wlanState = \"unknown\"; let wlanConn = \"\"; for (const l of lines) { const parts = l.split(\":\"); const dev = parts[0]; const state = parts[2]; const con = parts.slice(3).join(\":\"); if (dev === \"wlan0\") { wlanState = state; wlanConn = con; } } const apActive = (wlanState === \"connected\" && wlanConn === \"projekt-ap\"); const wifiConnected = (wlanState === \"connected\" && wlanConn && wlanConn !== \"projekt-ap\"); flow.set(\"apActive\", apActive); flow.set(\"wifiConnected\", wifiConnected); flow.set(\"wlanConn\", wlanConn); let statusText; if (wifiConnected) statusText = `WLAN verbunden (${wlanConn}). Projekt Dashboard aktiv.`; else if (apActive) statusText = \"Access Point aktiv (Projekt-Setup). Bitte WLAN einrichten. Passwort: 1234568\"; let qrUrl; let qrHint; if (apActive) { qrUrl = \"http://10.42.0.1:1880/ui/#!/1\"; qrHint = \"(Anmeldemaske)\"; } else { qrUrl = \"http://localhost:1880/ui/#!/1\"; qrHint = \"Projekt Dashboard öffnen\"; } flow.set(\"qrHint\", qrHint); return [ { payload: statusText }, { payload: qrUrl } ];",
    "outputs": 2,
    "x": 650,
    "y": 100,
    "wires": [
      [
        "ui_tpl_welcome_center"
      ],
      [
        "exec_qrencode"
      ]
    ]
  },
  {
    "id": "exec_qrencode",
    "type": "exec",
    "z": "tab_pa_final",
    "command": "/usr/bin/qrencode",
    "addpay": true,
    "append": "-t SVG -o -",
    "name": "QR erzeugen (qrencode SVG)",
    "x": 970,
    "y": 140,
    "wires": [
      [
        "fn_svg_to_dataurl"
      ],
      [
        "fn_qr_err"
      ],
      []
    ]
  },
  {
    "id": "fn_svg_to_dataurl",
    "type": "function",
    "z": "tab_pa_final",
    "name": "SVG -> DataURL (msg.payload) + Broadcast",
    "func": "const hint = flow.get(\"qrHint\") || \"\"; const out = (msg.payload || \"\").toString(); const idx = out.indexOf(\"<svg\"); let dataUrl = \"\"; if (idx >= 0) { const svg = out.slice(idx).trim(); const enc = encodeURIComponent(svg).replace(/'/g, \"%27\").replace(/\"/g, \"%22\"); dataUrl = `data:image/svg+xml;charset=utf-8,${enc}`; } msg.payload = { qrDataUrl: dataUrl, qrHint: hint }; return msg;",
    "outputs": 1,
    "x": 1350,
    "y": 120,
    "wires": [
      [
        "ui_tpl_qr_welcome"
      ]
    ]
